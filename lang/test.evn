def root = [
  look wowza.crazy.foo.bar
  look [
    background = 'black'
    foreground = 'light-gray'
    border = stuff
  ]

  show custom-button[text = 'hello world', other = 'wow']
  show custom-button[t = 'this works too, but it wont later!']
]

def custom-button[string text] = [
  look button
  look root

  show text
  show 'something'
]
