// this is a comment
// from 'standard' use button

root = ui[
  look wowza.crazy.foo.bar
  look [
    background = 'black'
    foreground = 'light-gray'
    border = stuff
  ]

  show custom-button[text = 'hello world', other = 'wow']
  show custom-button[t = 'this works too, but it wont later!']
]

custom-button[text is str] = ui[
  look button
  look root

  show text
  show 'something'

  boop = obj[
    choop = 'bloop'
    scoop = 'loop'
  ]
]

joe = obj[
  name = 'joe'
  age = '35'
]

more.stuff = wow[]

my-text[text is str] = ui[
  look green

  show text
]
